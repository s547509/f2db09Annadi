extends layout

block content
  h1= title 
  - var result = results
    div(class = 'container-fluid')
      div(class = 'row')
        each item,index in results 
          div(class="col-md-2", onClick=`buy(${index})`) icecreamName : #{item["icecreamName"]} <br> icecreamF : #{item["icecreamF"]} <br> icecreamPrice: #{item["icecreamPrice"]}
           script.
            function buy(val) {
              alert(`You purchased item ${val}`)
            }